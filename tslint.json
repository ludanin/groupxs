{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "defaultSeverity": "warning",
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "semicolon": true,
    "interface-name": false,
    "member-access": false,
    "quotemark": [true, "double"],
    "curly": false,
    "member-ordering": false,
    "no-empty": false
  },
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts"
    ]
  }
}
